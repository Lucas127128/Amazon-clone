{
    "compilerOptions": {
        "noEmit": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowImportingTsExtensions": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "module": "preserve",
        "target": "esnext",
        "strict": true
    }
}
{
    "compilerOptions": {
        "noEmit": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowImportingTsExtensions": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "target": "es2024",
        "module": "nodenext",
        "strict": true
    }
}
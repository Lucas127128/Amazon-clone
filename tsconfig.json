{
    "compilerOptions": {
        "noEmit": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowImportingTsExtensions": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "bundler",
        "module": "esnext",
        "target": "esnext",
        "strict": true,
        "skipLibCheck": true
    }
}
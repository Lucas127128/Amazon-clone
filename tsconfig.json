{
  "compilerOptions": {
    "noEmit": true,
    "preserveConstEnums": true,
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "bundler",
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "skipLibCheck": true,
    "types": ["trusted-types"]
  }
}

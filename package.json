{
  "scripts": {
    "test:data": "vitest tests/browser/data tests/normal/data",
    "test:api": "vitest tests/normal/api",
    "test:normal": "vitest tests/normal",
    "test": "vitest",
    "build": "vite build",
    "inspect": "node-modules-inspector",
    "build:backend": "bun build --compile --minify --sourcemap ./src/api/index.ts ",
    "api": "bun run build:backend && find . -name '*.bun-build' -type f -delete && NODE_TLS_REJECT_UNAUTHORIZED=0 ./api",
    "check:watch": "tsgo --noEmit --watch",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --fix --type-aware",
    "fmt": "oxfmt",
    "fmt:watch": "onchange '**' --exclude-path .gitignore -- bun run fmt",
    "request:orders": "cat ./tests/cart.json | xh post --https :3001/orders"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "axios": "^1.13.5",
    "elysia": "^1.4.25",
    "idb-keyval": "^6.2.2",
    "ky": "^1.14.3",
    "nanoid": "^5.1.6",
    "temporal-polyfill": "^0.3.0",
    "temporal-polyfill-lite": "^0.2.2",
    "ts-pattern": "^5.9.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/bun": "^1.3.9",
    "@types/k6": "^1.6.0",
    "@typescript/native-preview": "^7.0.0-dev.20260212.1",
    "@vitejs/devtools": "^0.0.0-alpha.31",
    "@vitest/browser-preview": "^4.0.18",
    "bun-types": "1.3.5",
    "happy-dom": "^20.6.1",
    "k6": "^0.0.0",
    "node-modules-inspector": "^1.3.2",
    "onchange": "^7.1.0",
    "oxfmt": "^0.32.0",
    "oxlint": "^1.47.0",
    "oxlint-tsgolint": "^0.12.1",
    "playwright": "^1.58.2",
    "sharp": "^0.34.5",
    "unplugin-macros": "^0.18.7",
    "unplugin-turbo-console": "^2.3.0",
    "vite": "^8.0.0-beta.14",
    "vite-plugin-html-minifier": "^1.0.5",
    "vite-plugin-remove-console": "^2.2.0",
    "vitest": "^4.0.18"
  }
